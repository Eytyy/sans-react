{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": [
					"last 2 versions",
					"not dead",
					"not < 2%"
				],
				"useBuiltIns": "entry",
				"corejs": "3.3.4"
			}
		],
		"@babel/preset-react"
	],
	"plugins": [
		"react-hot-loader/babel",
		"@babel/plugin-proposal-class-properties"
	]
}